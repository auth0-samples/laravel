{
    "name": "auth0-samples/laravel",
    "type": "project",
    "description": "Sample application demonstrating how to integrate Auth0 with Laravel 9 or Laravel 10 for session-based authentication and token-based authorization.",
    "keywords": [
        "framework",
        "laravel",
        "auth0",
        "authentication",
        "authorization",
        "login",
        "auth",
        "jwt",
        "json web token",
        "jwk",
        "json web key",
        "oauth",
        "openid",
        "secure",
        "protect",
        "api"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 0
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-create-project-cmd": [
            "rm -rf composer.json vendor editorconfig gitignore LICENSE sample/README.md",
            "composer install -d sample",
            "mv sample/* .",
            "rm -rf sample",
            "curl -sSfL https://raw.githubusercontent.com/auth0/auth0-cli/main/install.sh | sh -s -- -b ."
        ]
    }
}
